{
  "name": "allure-playwright",
  "version": "2.0.0-beta.19",
  "description": "Allure Playwright integration",
  "license": "Apache-2.0",
  "author": {
    "name": "Microsoft Corporation"
  },
  "repository": "https://github.com/allure-framework/allure-js",
  "keywords": [
    "playwright",
    "allure"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist ./out",
    "compile": "tsc",
    "generate-report": "allure generate ./out/allure-results -o ./out/allure-report --clean",
    "allure-report": "allure serve ./out/allure-results",
    "test": "playwright test -c test",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix"
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "allure-commandline": "^2.18.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "allure-js-commons": "../allure-js-commons"
  },
  "files": [
    "dist"
  ]
}
